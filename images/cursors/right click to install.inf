; shirokumacafe-panda
[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"

[Scheme.Cur]
normal.cur
help.cur
working.ani
busy.ani
text.cur
unavailable.cur
vertical.cur
horizontal.cur
diagonal.cur
diagonal2.cur
move.ani
link.ani
precision.cur
handwriting.cur
alternate.cur

[Scheme.Txt]

[Strings]
CUR_DIR       = "Cursors\shirokumacafe-panda"
SCHEME_NAME   = "shirokumacafe-panda"
pointer       = "normal.cur"
help          = "help.cur"
work          = "working.ani"
busy          = "busy.ani"
text          = "text.cur"
unavailiable  = "unavailable.cur"
vert          = "vertical.cur"
horz          = "horizontal.cur"
dgn1          = "diagonal.cur"
dgn2          = "diagonal2.cur"
move          = "move.ani"
link          = "link.ani"
cross         = "precision.cur"
hand          = "handwriting.cur"
alternate     = "alternate.cur"